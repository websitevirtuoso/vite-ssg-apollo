{
  "name": "dashboard",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "cypress": "vite --mode testing --port 1500",
    "serve": "vite preview",
    "vite:build": "vite build",
    "ssg:build": "vite-ssg build --debug",
    "vite:preview": "vite preview",
    "ssg:preview-https": "serve dist",
    "lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore --fix src",
    "format": "prettier --config ./.prettierrc .  --write",
    "postinstall": "esmo patches/patch.ts",
    "cy:run": "cypress run --config viewportWidth=1920,viewportHeight=1080",
    "cy:run:mobile": "cypress run --config viewportWidth=375,viewportHeight=667",
    "cy:open": "cypress open --config viewportWidth=1920,viewportHeight=1080",
    "cy:open:mobile": "cypress open --config viewportWidth=375,viewportHeight=667",
    "generate": "graphql-codegen --config codegen.yml -r dotenv/config"
  },
  "dependencies": {
    "@apollo/client": "^3.7.0",
    "@casl/ability": "^6.3.1",
    "@casl/vue": "^2.2.0",
    "@fawmi/vue-google-maps": "^0.9.79",
    "@tinymce/tinymce-vue": "^5.0.0",
    "@types/graphql": "^14.5.0",
    "@vue/apollo-composable": "^4.0.0-beta.1",
    "@vueuse/core": "^9.3.0",
    "@vueuse/head": "^0.9.7",
    "cropperjs": "^1.5.12",
    "dayjs": "^1.11.5",
    "graphql": "^16.6.0",
    "graphql-type-json": "^0.3.2",
    "patch-package": "^6.4.7",
    "pinia": "^2.0.22",
    "pinia-plugin-persistedstate": "^2.2.0",
    "roboto-fontface": "*",
    "vee-validate": "^4.6.10",
    "vue": "^3.2.40",
    "vue-3-breadcrumbs": "^2.0.0",
    "vue-i18n": "^9.2.2",
    "react": "^18.1.0",
    "vue-router": "^4.1.5",
    "vuedraggable": "^4.1.0",
    "vuetify": "^3.0.0-beta.13",
    "webfontloader": "^1.6.28",
    "yup": "^1.0.0-beta.7"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.5.0",
    "@graphql-codegen/cli": "2.13.5",
    "@graphql-codegen/typescript": "^2.7.3",
    "@intlify/vite-plugin-vue-i18n": "^6.0.3",
    "@mdi/js": "^7.0.96",
    "@modyqyw/vite-plugin-eslint": "^2.0.9",
    "@rollup/plugin-graphql": "^1.1.0",
    "@types/apollo-upload-client": "^17.0.1",
    "@types/google.maps": "^3.50.3",
    "@types/node": "^18.8.3",
    "@types/webfontloader": "^1.6.35",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vue/apollo-util": "^4.0.0-alpha.18",
    "@vue/eslint-config-typescript": "^11.0.2",
    "apollo-upload-client": "^17.0.0",
    "cypress": "^10.9.0",
    "cypress-vite": "^1.2.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-cypress": "^2.12.1",
